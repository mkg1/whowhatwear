<h1>Great, you already have an account. Please login.</h1>

<%= form_tag(login_path) do %>
  <p><%= label_tag :email %>
    <%= email_field_tag :email %> </p>
    <br>
    <p><%= label_tag :password %>
      <%= password_field_tag :password %></p>

    <div>
      <%= submit_tag "Log in" %>
    </div>
<% end %>
